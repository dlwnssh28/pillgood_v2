{"ast":null,"code":"import axios from '../axios';\nimport '../assets/styles.css';\nexport default {\n  name: 'MyCoupon',\n  data() {\n    return {\n      coupons: []\n    };\n  },\n  async created() {\n    await this.fetchCoupons();\n  },\n  methods: {\n    async fetchCoupons() {\n      try {\n        const response = await axios.get('/api/ownedcoupons/findbyid', {\n          withCredentials: true\n        });\n        this.coupons = response.data;\n        console.log('Fetched coupons:', this.coupons);\n      } catch (error) {\n        console.error('Failed to fetch coupons:', error);\n      }\n    },\n    formatDate(date) {\n      return new Date(date).toLocaleDateString();\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","coupons","created","fetchCoupons","methods","response","get","withCredentials","console","log","error","formatDate","date","Date","toLocaleDateString"],"sources":["C:\\Users\\woori\\IdeaProjects\\pillgood_v2\\src\\frontend\\src\\views\\MyCoupon.vue"],"sourcesContent":["<template>\r\n  <div class=\"main-container\">\r\n        <h2 class=\"text-melon\">보유 쿠폰</h2>\r\n    <div class=\"box-container-no-shade\">\r\n    <div v-if=\"coupons && coupons.length > 0\">\r\n      <table class=\"line-table\">\r\n        <thead>\r\n          <tr>\r\n            <th>쿠폰명</th>\r\n            <th>할인 금액</th>\r\n            <th>유효 기간</th>\r\n          </tr>\r\n        </thead>\r\n        <tbody>\r\n          <tr v-for=\"coupon in coupons\" :key=\"coupon.ownedCouponId\">\r\n            <td>{{ coupon.couponName }}</td>\r\n            <td>{{ coupon.discountAmount }}원</td>\r\n            <td>{{ formatDate(coupon.expiryDate) }}</td>\r\n          </tr>\r\n        </tbody>\r\n      </table>\r\n    </div>\r\n    <div v-else>\r\n      <p>보유한 쿠폰이 없습니다.</p>\r\n    </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from '../axios';\r\nimport '../assets/styles.css';\r\n\r\nexport default {\r\n  name: 'MyCoupon',\r\n  data() {\r\n    return {\r\n      coupons: []\r\n    };\r\n  },\r\n  async created() {\r\n    await this.fetchCoupons();\r\n  },\r\n  methods: {\r\n    async fetchCoupons() {\r\n      try {\r\n        const response = await axios.get('/api/ownedcoupons/findbyid', { withCredentials: true });\r\n        this.coupons = response.data;\r\n        console.log('Fetched coupons:', this.coupons);\r\n      } catch (error) {\r\n        console.error('Failed to fetch coupons:', error);\r\n      }\r\n    },\r\n    formatDate(date) {\r\n      return new Date(date).toLocaleDateString();\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.main-container {\r\n  padding: 20px;\r\n}\r\n\r\n.line-table {\r\n  width: 100%;\r\n  border-collapse: collapse;\r\n}\r\n\r\n.line-table th,\r\n.line-table td {\r\n  border: 1px solid #ddd;\r\n  padding: 8px;\r\n  text-align: left;\r\n}\r\n\r\n.line-table th {\r\n  background-color: #f4f4f4;\r\n}\r\n\r\n.box-shadow {\r\n  box-shadow: 0 4px 8px rgba(0,0,0,0.1);\r\n}\r\n</style>\r\n"],"mappings":"AA8BA,OAAOA,KAAI,MAAO,UAAU;AAC5B,OAAO,sBAAsB;AAE7B,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE;IACX,CAAC;EACH,CAAC;EACD,MAAMC,OAAOA,CAAA,EAAG;IACd,MAAM,IAAI,CAACC,YAAY,CAAC,CAAC;EAC3B,CAAC;EACDC,OAAO,EAAE;IACP,MAAMD,YAAYA,CAAA,EAAG;MACnB,IAAI;QACF,MAAME,QAAO,GAAI,MAAMP,KAAK,CAACQ,GAAG,CAAC,4BAA4B,EAAE;UAAEC,eAAe,EAAE;QAAK,CAAC,CAAC;QACzF,IAAI,CAACN,OAAM,GAAII,QAAQ,CAACL,IAAI;QAC5BQ,OAAO,CAACC,GAAG,CAAC,kBAAkB,EAAE,IAAI,CAACR,OAAO,CAAC;MAC/C,EAAE,OAAOS,KAAK,EAAE;QACdF,OAAO,CAACE,KAAK,CAAC,0BAA0B,EAAEA,KAAK,CAAC;MAClD;IACF,CAAC;IACDC,UAAUA,CAACC,IAAI,EAAE;MACf,OAAO,IAAIC,IAAI,CAACD,IAAI,CAAC,CAACE,kBAAkB,CAAC,CAAC;IAC5C;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}