{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { mapGetters, mapActions, mapState } from 'vuex';\nimport '../assets/styles.css';\nexport default {\n  name: 'MyInquiries',\n  computed: {\n    ...mapState('member', ['member']),\n    ...mapGetters('inquiry', ['allInquiries']),\n    inquiries() {\n      return this.allInquiries || [];\n    }\n  },\n  methods: {\n    ...mapActions('inquiry', ['fetchInquiriesByMember']),\n    formatDate(date) {\n      const options = {\n        year: 'numeric',\n        month: '2-digit',\n        day: '2-digit'\n      };\n      return new Date(date).toLocaleDateString(undefined, options);\n    },\n    async fetchInquiries() {\n      if (this.member && this.member.memberUniqueId) {\n        try {\n          await this.fetchInquiriesByMember(this.member.memberUniqueId);\n          console.log('문의 조회 성공');\n        } catch (error) {\n          console.error('문의 조회 실패:', error);\n        }\n      } else {\n        console.error('회원 정보가 없습니다.');\n      }\n    },\n    goToInquiryCreate() {\n      this.$router.push('/inquiries/create');\n    }\n  },\n  async created() {\n    await this.fetchInquiries();\n  }\n};","map":{"version":3,"names":["mapGetters","mapActions","mapState","name","computed","inquiries","allInquiries","methods","formatDate","date","options","year","month","day","Date","toLocaleDateString","undefined","fetchInquiries","member","memberUniqueId","fetchInquiriesByMember","console","log","error","goToInquiryCreate","$router","push","created"],"sources":["C:\\Users\\woori\\IdeaProjects\\pillgood_v2\\src\\frontend\\src\\views\\MyInquiries.vue"],"sourcesContent":["<template>\n  <div class=\"main-container box-shadow\">\n    <h2 class=\"text-melon\">나의 문의</h2>\n    <div class=\"box-container-no-shade\">\n      <div v-if=\"inquiries && inquiries.length > 0\"> \n        <table class=\"line-table\">\n          <thead>\n            <tr>\n              <th>제목</th>\n              <th>유형</th>\n              <th>상태</th>\n              <th>작성일</th>\n            </tr>\n          </thead>\n          <tbody>\n            <tr v-for=\"inquiry in inquiries\" :key=\"inquiry.inquiryNo\">\n              <td>\n                <router-link :to=\"{ name: 'InquiryDetail', params: { id: inquiry.inquiryNo } }\">\n                  {{ inquiry.inquiryTitle }}\n                </router-link>\n              </td>\n              <td>{{ inquiry.inquiryType }}</td>\n              <td>{{ inquiry.inquiryStatus }}</td>\n              <td>{{ formatDate(inquiry.inquiryDate) }}</td>\n            </tr>\n          </tbody>\n        </table>\n      </div>\n      <div v-else>\n        <p>문의가 없습니다.</p>\n      </div>\n        <div class=\"btn-container\" style=\"margin-top: 10px;\">\n        <button @click=\"goToInquiryCreate\" class=\"btn btn-green\">글쓰기</button>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { mapGetters, mapActions, mapState } from 'vuex';\nimport '../assets/styles.css';\n\nexport default {\n  name: 'MyInquiries',\n  computed: {\n    ...mapState('member', ['member']),\n    ...mapGetters('inquiry', ['allInquiries']),\n    inquiries() {\n      return this.allInquiries || [];\n    }\n  },\n  methods: {\n    ...mapActions('inquiry', ['fetchInquiriesByMember']),\n    formatDate(date) {\n      const options = { year: 'numeric', month: '2-digit', day: '2-digit' };\n      return new Date(date).toLocaleDateString(undefined, options);\n    },\n    async fetchInquiries() {\n      if (this.member && this.member.memberUniqueId) {\n        try {\n          await this.fetchInquiriesByMember(this.member.memberUniqueId);\n          console.log('문의 조회 성공');\n        } catch (error) {\n          console.error('문의 조회 실패:', error);\n        }\n      } else {\n        console.error('회원 정보가 없습니다.');\n      }\n    },\n    goToInquiryCreate() {\n      this.$router.push('/inquiries/create');\n    }\n  },\n  async created() {\n    await this.fetchInquiries();\n  }\n};\n</script>\n\n<style scoped>\